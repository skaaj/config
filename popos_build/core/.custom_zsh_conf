export WORKSPACE_DIR="/home/skaaj/Workspace"
export WS=$WORKSPACE_DIR

function browse() {
    directory=`ls -Rd $1/* | fzf`
    if [ -z "$directory" ]
    then
     directory=$1
    fi
    cd $directory;
}

alias p='cd $(find $WORKSPACE_DIR -maxdepth 2 -type d | fzf);'
alias open=xdg-open
